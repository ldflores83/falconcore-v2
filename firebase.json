{
  "hosting": [
    {
      "target": "uaylabs",
      "public": "frontends/uaylabs/out",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "headers": [
        {
          "source": "**/*.js",
          "headers": [
            {
              "key": "Content-Type",
              "value": "application/javascript"
            }
          ]
        }
      ],
      "rewrites": [
        {
          "source": "/onboardingaudit/api/**",
          "function": "api",
          "region": "us-central1"
        },
        {
          "source": "/ignium/api/**",
          "function": "api",
          "region": "us-central1"
        },
        {
          "source": "/jobpulse/api/**",
          "function": "api",
          "region": "us-central1"
        },
        {
          "source": "/pulziohq/api/**",
          "function": "api",
          "region": "us-central1"
        },
        {
          "source": "/ignium/**",
          "destination": "/ignium/index.html"
        },
        {
          "source": "/jobpulse/**",
          "destination": "/jobpulse/index.html"
        },
        {
          "source": "/pulziohq/**",
          "destination": "/pulziohq/index.html"
        },
        {
          "source": "/onboardingaudit/login",
          "destination": "/onboardingaudit/login/index.html"
        },
        {
          "source": "/onboardingaudit/login/**",
          "destination": "/onboardingaudit/login/index.html"
 
       },
        {
          "source": "/onboardingaudit/admin",
          "destination": "/onboardingaudit/admin/index.html"
        },
        {
          "source": "/onboardingaudit/admin/**",
          "destination": "/onboardingaudit/admin/index.html"
        },
        {
          "source": "/onboardingaudit/**",
          "destination": "/onboardingaudit/index.html"
        },
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ],
  "functions": {
    "source": "functions",
    "runtime": "nodejs20",
    "secrets": ["cloud-storage-key", "ENCRYPTION_KEY"]
  },
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}
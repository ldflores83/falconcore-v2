{
  "hosting": [
    {
      "target": "uaylabs",
      "public": "frontends/uaylabs/out",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "headers": [
        {
          "source": "**/*.js",
          "headers": [
            {
              "key": "Content-Type",
              "value": "application/javascript"
            }
          ]
        }
      ],
      "rewrites": [
        {
          "source": "/ignium/**",
          "destination": "/ignium/index.html"
        },
        {
          "source": "/jobpulse/**",
          "destination": "/jobpulse/index.html"
        },
        {
          "source": "/pulziohq/**",
          "destination": "/pulziohq/index.html"
        },
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ],
  "functions": {
    "source": "functions",
    "runtime": "nodejs20"
  }
}